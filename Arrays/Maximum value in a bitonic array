#include<stdio.h>
int main()
{
	//code
	int t,n,arr[1000001],i;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%d",&n);
	    for(i=0;i<n;i++)
	    {
	        scanf("%d",&arr[i]);
	    }
	    for(i=0;i<n-1;i++)
	    {
	        if(arr[i]>arr[i+1])
	        {
	            break;
	        }
	    }
	    printf("%d\n",arr[i]);
	}
	return 0;
}
